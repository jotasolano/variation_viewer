{
	"name": "variation_viewer",
	"description": "",
	"version": "0.0.28",
	"threevot": {
		"version": "28",
		"screens": {
			"0-320": "main",
			"320-900": "main",
			"900-1200": "main",
			"1200-5000": "main"
		},
		"extensions": [
			".coffee",
			".eco",
			".html"
		],
		"transforms": [
			"browserify-eco",
			"brfs",
			"uglifyify"
		],
		"external": {},
		"gitDependencies": {},
